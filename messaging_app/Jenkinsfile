                }
            }
        }
        // ... (ÿ®ÿßŸÇŸä ÿßŸÑŸÖÿ±ÿßÿ≠ŸÑ ŸÉŸÖÿß ŸáŸä)     
        
        // === ÿßŸÑŸÖÿ±ÿ≠ŸÑÿ© 3: ÿ®ŸÜÿßÿ° ÿµŸàÿ±ÿ© Docker (ÿßŸÑŸÖŸáŸÖÿ© 1) ===
        stage('3. Build Docker Image') {
            steps {
                dir('messaging_app') {
                    script {
                        // ÿ®ŸÜÿßÿ° ÿßŸÑÿµŸàÿ±ÿ©
                        docker.build("${DOCKER_IMAGE_NAME}:${env.BUILD_NUMBER}", ".")
                    }
                }
            }
        }
        
        // === ÿßŸÑŸÖÿ±ÿ≠ŸÑÿ© 4: ÿ±ŸÅÿπ ÿßŸÑÿµŸàÿ±ÿ© ŸÑŸÄ Docker Hub (ÿßŸÑŸÖŸáŸÖÿ© 1) ===
        stage('4. Push to Docker Hub') {
            steps {
                script {
                    // ÿßÿ≥ÿ™ÿÆÿØÿßŸÖ ÿßŸÑÿ£ÿ≥ÿ±ÿßÿ± ŸÑÿ±ŸÅÿπ ÿßŸÑÿµŸàÿ±ÿ©
                    docker.withRegistry('https://registry.hub.docker.com', DOCKER_CREDS) {
                        docker.image("${DOCKER_IMAGE_NAME}:${env.BUILD_NUMBER}").push()
                        docker.image("${DOCKER_IMAGE_NAME}:latest").push()
                    }
                }
            }
        }
    }
